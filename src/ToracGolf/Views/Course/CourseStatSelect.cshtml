@{
    Layout = "_ToracGolfLayout";
    ViewBag.Title = "Course Stats";
}
@model  ToracGolf.ViewModels.Courses.CourseStatsViewModel

@section Breadcrumb{
    @Html.Partial("_Breadcrumb", Model.Breadcrumb)
}

@section TopNav{
    @Html.Partial("_TopNav", new ToracGolf.ViewModels.Navigation.TopNavViewModel(true))
}

@section HandicapStatus{
    @Html.Partial("_HandicapStatus", Model.HandicapStatus)
}

@section Styles{
    <link href="~/css/courses.css" rel="stylesheet" />
}

@section Scripts{
    <script src="~/js/common/ajaxcommon.js"></script>
    <script src="~/lib/toraclibrary/toracajax.js"></script>
    <script src="~/lib/react/react.js"></script>
    <script src="~/lib/react/react-dom.js"></script>
    <script src="~/js/react/courses/coursestats.js"></script>
}

<section id="CourseStatsSection" class="slice slice-lg bg-image">

    <section class="slice bg-white bb">
        <div class="wp-section estate">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12"><h3>@Model.CourseRecord.CourseData.Name</h3></div>
                </div>
                <div class="row">
                    <div class="col-md-9">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="product-gallery">
                                    <div class="primary-image">
                                        <a href="images/prv/estate/item-1.jpg" class="theater" rel="group" hidefocus="true">
                                            <img src="data:image/png;base64,@Convert.ToBase64String(Model.CourseRecord.CourseImage)" class="img-responsive" alt="Course Image">
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="product-info">
                                    <div class="wp-block property list no-border">
                                        <div class="wp-block-content clearfix">
                                            <h4 class="content-title">@Model.CourseRecord.CourseData.City, @Model.CourseRecord.CourseState</h4>
                                            <p class="description mb-15">@Model.CourseRecord.CourseData.Description</p>
                                            <span class="pull-left">
                                                <span class="price">$230</span>
                                                <span class="period">per month</span>
                                            </span>
                                            <span class="pull-right">
                                                <span class="capacity">
                                                    <i class="fa fa-user"></i>
                                                    <i class="fa fa-user"></i>
                                                </span>
                                            </span>
                                        </div>
                                        <div class="wp-block-footer style2 mt-15">
                                            <ul class="aux-info">
                                                <li><i class="fa fa-building"></i>Tee Boxes: @Model.CourseRecord.TeeBoxCount</li>
                                                <li><i class="fa fa-user"></i>Rounds: @Model.CourseRecord.RoundCount</li>
                                                <li><i class="fa fa-tint"></i>Best Score: @Model.CourseRecord.BestScore</li>
                                                <li><i class="fa fa-car"></i>Avg Score: @Model.CourseRecord.AverageScore.ToString("n1")</li>
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="rating pull-left">
                                        <span class="star voted" rel="1"></span>
                                        <span class="star voted" rel="2"></span>
                                        <span class="star voted" rel="3"></span>
                                        <span class="star voted" rel="4"></span>
                                        <span class="star" rel="5"></span>
                                    </div>
                                    <span class="review-rating pull-left" style="margin-left:15px;">
                                        <a href="" hidefocus="true">Read all 23 reviews</a> or
                                        <a href="" hidefocus="true">Share you oppinion</a>

                                    </span>
                                    <span class="clearfix"></span>
                                    <p>
                                        Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.
                                    </p>

                                </div>
                            </div>
                        </div>

                        <!-- PROPERTY DESCRIPTION -->
                        <div class="row">
                            <div class="col-md-12">
                                <div class="tabs-framed boxed">
                                    <ul class="tabs clearfix">
